{
  "name": "reroche",
  "description": "Ricardo Elias Roche's personal website",
  "version": "2.1.3",
  "author": "Ricardo Elias Roche <rers97@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.271",
    "gatsby-image": "^1.0.53",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-sharp": "^1.6.47",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^1.5.38",
    "gatsby-transformer-remark": "^1.7.43",
    "gatsby-transformer-sharp": "^1.6.26",
    "react-entypo-icons": "^1.4.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && cp -r src/docs public && echo '<-- Copied docs to public folder! -->'",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && gatsby build --prefix-paths && gh-pages -d public -b master",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write '*.js'",
    "format-code": "npm run prettier && npm run lint:fix",
    "precommit": "lint-staged",
    "build-serve": "npm run build && gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn run format-code",
      "git add"
    ]
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "prettier": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
